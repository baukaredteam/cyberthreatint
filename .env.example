# Настройки Telegram бота
TELEGRAM_BOT_TOKEN=YOUR_BOT_TOKEN_HERE
TELEGRAM_CHAT_ID=YOUR_CHAT_ID_HERE

# URL для подключения к Certstream
CERTSTREAM_URL=wss://certstream.calidog.io

# Путь к базе данных SQLite
DATABASE_PATH=./data/phishing.db

# Уровень логирования (debug, info, warn, error)
LOG_LEVEL=info

# Домены клиентов для мониторинга (через запятую)
CLIENT_DOMAINS=qazpost.kz,example.com,client1.com,client2.com

# Интервал обновления баз данных (в минутах)
UPDATE_INTERVAL=60

# Порог схожести доменов (0.0 - 1.0)
SIMILARITY_THRESHOLD=0.8